{
  "name": "my-app",
  "version": "0.2.0",
  "private": true,
  "homepage": "https://llouisetaylor.com",
  "dependencies": {
    "core-js": "^3.6.5",
    "express": "^4.17.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-inlinesvg": "^0.8.4",
    "react-scripts": "^3.4.1",
    "svg-inline-react": "^3.2.0"
  },
  "scripts": {
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js && copyfiles README.md robots.txt build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build-css": "node-sass-chokidar src/ -o src/styles",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/styles --watch --recursive"
  },
  "devDependencies": {
    "ajv": "^6.12.3",
    "copyfiles": "^1.2.0",
    "fibers": "^4.0.3",
    "morgan": "^1.10.0",
    "node-sass-chokidar": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.26.10"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
